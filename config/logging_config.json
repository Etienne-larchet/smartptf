{
    "version": 1,
    "disable_existing_loggers": true,
    "formatters": {
      "short": {
        "format": "%(levelname)s - %(name)s - %(message)s"
      }
    },
    "handlers": {
      "console": {
        "level": "DEBUG",
        "class": "logging.StreamHandler",
        "formatter": "short"
      }
    },
    "loggers": {
      "smartptf": {
        "level": "DEBUG",
        "handlers": ["console"]
      }
    }
  }
  